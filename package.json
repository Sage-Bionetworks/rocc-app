{
  "name": "@sage-bionetworks/rocc-app",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/environments/setenv.ts",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build:rocc-client-angular": "cd rocc-client-angular && npm ci && npm run build",
    "build:sage-angular": "cd sage-angular && npm ci && npm run build",
    "install:dependencies": "npm run build:rocc-client-angular && npm run build:sage-angular && npm ci",
    "install:deps": "npm run install:dependencies",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/rocc-app/stats.json",
    "release": "release-it"
  },
  "engines": {
    "node": ">=14.16.1",
    "npm": ">=6.14.12"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/material": "^12.1.4",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@sage-bionetworks/rocc-client-angular": "file:rocc-client-angular/dist/rocc-client",
    "@sage-bionetworks/sage-angular": "file:sage-angular/dist/sage-angular",
    "deep-equal": "^1.1.1",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/jasmine": "~3.7.8",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.4.10",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "release-it": "^14.10.0",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
