{
  "name": "@sage-bionetworks/rocc-app",
  "version": "0.6.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/environments/setenv.ts",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build:rocc-client-angular": "cd rocc-client-angular && npm ci --legacy-peer-deps && npm run build",
    "build:sage-angular": "cd sage-angular && npm ci --legacy-peer-deps && npm run build",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test:e2e": "jest --config='e2e/jest-e2e.config.js'",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/rocc-app/stats.json",
    "release": "release-it",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "preinstall": "npm run build:rocc-client-angular && npm run build:sage-angular"
  },
  "engines": {
    "node": ">=14.16.1",
    "npm": ">=6.14.12"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.0",
    "@angular/cdk": "^13.2.0",
    "@angular/common": "^13.2.0",
    "@angular/compiler": "^13.2.0",
    "@angular/core": "^13.2.0",
    "@angular/forms": "^13.2.0",
    "@angular/material": "^13.2.0",
    "@angular/platform-browser": "^13.2.0",
    "@angular/platform-browser-dynamic": "^13.2.0",
    "@angular/router": "^13.2.0",
    "@sage-bionetworks/rocc-client-angular": "file:rocc-client-angular/dist/rocc-client",
    "@sage-bionetworks/sage-angular": "file:sage-angular/dist/sage-angular",
    "deep-equal": "^2.0.5",
    "dotenv": "^15.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "ngx-avatar": "^4.1.0",
    "ngx-countup": "^7.3.3",
    "ngx-typed-js-public": "^1.2.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.1.0",
    "rxjs": "^7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.1",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.2.1",
    "@angular/compiler-cli": "^13.2.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jasmine": "~3.10.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "release-it": "^14.12.4",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.5",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ]
  }
}
